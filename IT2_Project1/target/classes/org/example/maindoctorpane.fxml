<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<TabPane fx:controller="org.example.Controller" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
         minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE"
         xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1">
    <Tab text="Temperature">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <FlowPane layoutX="354.0" layoutY="84.0" prefHeight="200.0" prefWidth="200.0"
                      style="-fx-background-color: #FAFAFA#FAFAFA;"/>
            <Label layoutX="14.0" layoutY="31.0" text="TEMPERATUR:">
                <font>
                    <Font size="24.0"/>
                </font>
            </Label>
            <Button fx:id="starttemperature" layoutX="14.0" layoutY="84.0" mnemonicParsing="false"
                    onAction="#starttemperature" text="Start Måling"/>
            <Button fx:id="stoptemperature" layoutX="14.0" layoutY="123.0" mnemonicParsing="false"
                    onAction="#stoptemperature" text="Stop Måling"/>
            <Button fx:id="setlowerboundstemperature" layoutX="14.0" layoutY="257.0" mnemonicParsing="false"
                    onAction="#setlowerboundstemperature" text="Nedre Grænse"/>
            <Button fx:id="setupperboundstemperature" layoutX="14.0" layoutY="221.0" mnemonicParsing="false"
                    onAction="#setupperboundstemperature" prefHeight="27.0" prefWidth="98.0"
                    text="Øvre Grænse"/>
        </AnchorPane>
    </Tab>
    <Tab text="Puls">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <FlowPane layoutX="354.0" layoutY="84.0" prefHeight="200.0" prefWidth="200.0"
                      style="-fx-background-color: #FAFAFA#FAFAFA;"/>
            <Label layoutX="14.0" layoutY="31.0" text="PULS:">
                <font>
                    <Font name="System Bold" size="24.0"/>
                </font>
            </Label>
            <Button fx:id="startpulse" layoutX="14.0" layoutY="84.0" mnemonicParsing="false"
                    onAction="#startpulse" text="Start Måling"/>
            <Button fx:id="lowerboundspulse" layoutX="14.0" layoutY="257.0" mnemonicParsing="false"
                    onAction="#lowerboundspulse" text="Nedre Grænse"/>
            <Button fx:id="stopspulse" layoutX="14.0" layoutY="124.0" mnemonicParsing="false"
                    onAction="#stoppulse" text="Sltop Måling"/>
            <Button fx:id="upperboundspulse" layoutX="14.0" layoutY="217.0" mnemonicParsing="false"
                    onAction="#upperboundspulse" prefHeight="27.0" prefWidth="98.0" text="Øvre Grænse"/>
        </AnchorPane>
    </Tab>
    <Tab text="Ilt">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <FlowPane layoutX="354.0" layoutY="84.0" prefHeight="200.0" prefWidth="200.0"
                      style="-fx-background-color: #FAFAFA#FAFAFA;"/>
            <Label layoutX="14.0" layoutY="31.0" text="SATURATION:">
                <font>
                    <Font name="System Bold" size="24.0"/>
                </font>
            </Label>
            <Button fx:id="startsaturation" layoutX="14.0" layoutY="84.0" mnemonicParsing="false"
                    onAction="#startsaturation" text="Start Måling"/>
            <Button fx:id="stopsaturation" layoutX="15.0" layoutY="126.0" mnemonicParsing="false"
                    onAction="#stopsaturation" text="Stop Måling"/>
            <Button fx:id="setupperboundssaturation" layoutX="15.0" layoutY="217.0" mnemonicParsing="false"
                    onAction="#setupperboundssaturation" prefHeight="27.0" prefWidth="96.0" text="Øvre Grænse"/>
            <Button fx:id="setlowerboundssaturation" layoutX="15.0" layoutY="257.0" mnemonicParsing="false"
                    onAction="#setlowerboundssaturation" text="Nedre grænse"/>
        </AnchorPane>
    </Tab>
    <Tab text="EKG">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <FlowPane layoutX="-1.0" layoutY="68.0" prefHeight="300.0" prefWidth="600.0"
                      style="-fx-background-color: #FAFAFA#FAFAFA;"/>
            <Button fx:id="startecg" layoutX="329.0" layoutY="32.0" mnemonicParsing="false" onAction="#startecg"
                    prefHeight="27.0" prefWidth="122.0" text="Start Måling"/>
            <Button fx:id="stopecg" layoutX="464.0" layoutY="32.0" mnemonicParsing="false" onAction="#stopecg"
                    prefHeight="27.0" prefWidth="122.0" text="Stop"/>
            <Label layoutX="14.0" layoutY="31.0" text="EKG :">
                <font>
                    <Font name="System Bold" size="24.0"/>
                </font>
            </Label>
        </AnchorPane>
    </Tab>
</TabPane>